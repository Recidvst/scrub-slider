{
  "name": "scrub-slider",
  "version": "2.0.1",
  "description": "Scrub Slider",
  "main": "dist/scrub.cjs.js",
  "module": "dist/scrub.esm.js",
  "browser": "dist/scrub.umd.js",
  "umd": "dist/scrub.umd.js",
  "iife": "dist/scrub.umd.js",
  "css": "dist/scrub.css",
  "styles": "dist/scrub.css",
  "scripts": {
    "prettier:fix": "npx prettier . --write",
    "build": "cross-env BABEL_ENV=production NODE_ENV=production rollup -c && npm run example:css && npm run example:js",
    "example:css": "node-sass src/scss/scrub.scss -o example/css && node-sass src/scss/example.scss -o example/css && cleancss example/css/example.css -o example/css/example.css",
    "example:js": "babel src/js/example.js --out-file example/example.js",
    "package:pack": "npm pack",
    "package:minor": "npm version minor",
    "package:patch": "npm version patch",
    "postinstall": "npx husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Recidvst/scrub-slider.git"
  },
  "author": "Chris Snowden <cdsnowden@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Recidvst/scrub-slider/issues"
  },
  "keywords": [
    "slider",
    "carousel",
    "slideshow",
    "javascript",
    "gallery",
    "lightweight",
    "vanilla",
    "responsive",
    "scrubber",
    "scrub"
  ],
  "homepage": "https://github.com/Recidvst/scrub-slider#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/register": "^7.22.15",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "autoprefixer": "^10.4.16",
    "babel-plugin-add-module-exports": "^1.0.4",
    "bootstrap-grid": "^3.0.1",
    "clean-css-cli": "^5.6.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "node-sass": "^9.0.0",
    "npm-check-updates": "^16.14.6",
    "prettier": "^3.0.3",
    "rollup": "^4.1.4",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "sass": "^1.69.3"
  },
  "lint-staged": {
    "*": "npx prettier . --write"
  }
}
