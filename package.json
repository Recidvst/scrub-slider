{
  "name": "scrub-slider",
  "version": "1.1.1",
  "description": "Scrub Slider",
  "source": "js/scrub.js",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "browser": "dist/browser.js",
  "css": "dist/styles.css",
  "scripts": {
    "prettier:fix": "npx prettier . --write",
    "build": "parcel build --dist-dir dist",
    "dev": "parcel index.html --dist-dir local",
    "package:pack": "npm pack",
    "package:minor": "npm version minor",
    "package:patch": "npm version patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Recidvst/scrub-slider.git"
  },
  "author": "Chris Snowden <cdsnowden@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Recidvst/scrub-slider/issues"
  },
  "homepage": "https://github.com/Recidvst/scrub-slider#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.10.0",
    "bootstrap-grid": "^2.0.1",
    "husky": "^8.0.3",
    "npm-check-updates": "^16.14.5",
    "parcel": "^2.10.0",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3"
  },
  "browserslist": "defaults, maintained node versions, IE >= 9",
  "sideEffects": "true",
  "targets": {
    "css": {
      "source": "src/scss/scrub.scss",
      "optimize": true
    },
    "browser": {
      "source": "src/js/scrub.js",
      "optimize": false
    },
    "module": {
      "source": "src/js/scrub.js",
      "optimize": false
    },
    "main": {
      "source": "src/js/scrub.js",
      "optimize": false
    }
  }
}
